    "\n",
    "# Plot the normal distribution\n",
    "plt.plot(x, pdf, 'b-', lw=2)\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('Probability Density')\n",
    "plt.title('Normal Distribution (μ = 0, σ = 1)')\n",
    "plt.show()\n"
   ]
